package ejercicio2;

public class principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		/*Piscina rectangular 
		 */
		double largo= 50; 
		double alto= 21;
		double ancho= 2.5;
		double resultado;
		/* Piscina redonda 
		 */
		double altoC=1.80, radio=12, exp=2; 
		double resultadoC;
		
		resultadoC=Math.PI*Math.pow(radio, exp)*altoC;
		  
		resultado=alto*largo*ancho;
		
		
		
		
		System.out.println("Hola, el resultado de la primera piscina rectangular es: " +resultado);
		resultado=alto*largo*ancho+" metro cubico";
		
		System.out.println("Hola, el resultado de la piscina circular es: "+ resultadoC);
		
		
		
		
		

	}

}
